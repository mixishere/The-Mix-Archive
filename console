(function () {
    const log = console.log;
    log("ðŸ§  Console module initialized.");
    log("Type 'help()' for available commands.");

    window.help = function () {
        log("Available commands:");
        log("- help(): Show this list");
        log("- status(): Display system status");
        log("- clear(): Clear the console");
    };

    window.status = function () {
        log("Aurora Core: ONLINE");
        log("Modules Loaded: disk, clickr, dread");
        log("Memory Sync: ACTIVE");
    };

    window.clear = function () {
        console.clear();
        log("ðŸ§  Console cleared.");
    };
})();
